{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "out",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"resolveJsonModule": true,
		"baseUrl": "./src",
		"paths": {
			"@services/*" : ["./services/*"],
			"@objects/*" : ["./objects/*"],
			"@resources/*" : ["./resources/*"],
			"@assets/*" : ["./assets/*"],
			"@examples/*" : ["./assets/examples/*"]
		}
	}
}
